{% extends 'base.html.twig' %}
{%block title %} Sommere | {{ parent() }}{% endblock %}
{% block body %}
<!DOCTYPE html>
<html>
<body>
    <div class="justify-content-between align-items-center" style="margin-left: 10% ">
        <p>
            <a href="{{path('app_index')}}"> <h5>Index</h5>
            </a>
        </p>
        <h2>
            <strong> Sommere </strong>
        </h2>
    <p>
        <div class="col-12 col-md-8 ps-5 " >
            <h2 class="mb-4">La somme des meres</h2>
            {% for zone in zones %}          
                <div class="d-flex mb-3 line-list">
                    <a href="#"
                    <span class="line-details">{{ zone.ligne }} - {{ zone.zones }}</span>
                </a>
                </div>
            {% endfor %}
        </div>
    </p>
</div>
</body>
</html>
{% endblock %}