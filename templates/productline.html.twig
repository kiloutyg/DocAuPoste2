{% extends 'base.html.twig' %}
{% block title %}
	{{productLine.name}}
	|
	{{ parent() }}
{% endblock %}
{% block body %}
	<!DOCTYPE html>
	<html>
		<body>
			<div class="justify-content-between align-items-center" style="margin-left: 10% ">

				<h2>
					<strong>
						{{ productLine.name }}
					</strong>
				</h2>
				<p>
					<div class="col-12 col-md-8 ps-5 mb-5 mt-5 ">
						<h2 class="mb-4">{{productLine.name}}</h2>
						{{ dump(productLine) }}
						{{ dump(productLine.zone.id) }}

					</p>
				</p>
			</p>
		</body>
	</html>
{% endblock %}
