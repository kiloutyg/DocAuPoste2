{% extends 'base.html.twig' %}
{%block title %}
	Sommere |
	{{ parent() }}
{% endblock %}
{% block body %}
	<!DOCTYPE html>
	<html>
		<body>
			<div class="justify-content-between align-items-center" style="margin-left: 10% ">

				<h2>
					<strong>
						Sommere
					</strong>
				</h2>
				<p>
					<div class="col-12 col-md-8 ps-5 mb-5 mt-5 ">
						<h2 class="mb-4">La somme des meres</h2>

						{{ dump(productLines) }}
						{% for productLine in productLines %}

							<div class="d-flex ps-5 mb-5 mt-5  line-list">
								<a{{productLine.name}} href="#" <span class="line-details"> - {{ productLine.name }} <id={{productLine.name}}>></span>
							</a>
						</div>
					{% endfor %}
				</p>
			</div>
		</body>
	</html>
</p></div></body></html>{% endblock %}
